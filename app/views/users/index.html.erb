<%= link_to "New Upload" ,new_user_path%>

<table>
	<% @uploads.each do |upload|%>
	<tr>
		<th>Name</th>
		<td><%= upload.name %></td>
	</tr>
	<tr>
		<th>List of images</th>
		<td>
			<ul>
			    <% upload.attachments.each do |attachment| %>
			      <li><%== link_to(image_tag(attachment.url, alt: 'Image'), attachment.url, target: '_blank')%></li>
			    <% end %>
		  	</ul>
	  	</td>
  	</tr>
<%end%>
</table>
