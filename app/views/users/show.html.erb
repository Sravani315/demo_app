
<p><strong>Name:</strong><%=@upload.name%></p>
<% if @upload.attachments? %>
  <ul>
    <% @upload.attachments.each do |attachment| %>
      <li><%=link_to(image_tag(attachment.url, alt: 'pdf', width: '50', height: '50'), attachment.url, target: '_self') %></li>
    <% end %>
  </ul>
<% end %>
